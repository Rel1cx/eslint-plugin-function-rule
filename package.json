{
  "name": "eslint-plugin-function-rule",
  "version": "0.0.16",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "format:check": "dprint check",
    "format:write": "dprint fmt",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/core": "^1.0.0",
    "dprint": "^0.50.2",
    "eslint": "^9.39.1",
    "tsdown": "^0.16.4",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@eslint/core": "^1.0.0",
    "eslint": "^9.39.1"
  }
}
